<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="startMenuPane" alignment="CENTER" disable="false" gridLinesVisible="false" prefHeight="312.0" prefWidth="396.0" scaleX="1.0" visible="true" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="StartMenuController">
  <children>
    <Label fx:id="titleLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="46.0" prefWidth="160.0" text="Final Fantasy Clone" textAlignment="CENTER" underline="false" GridPane.columnIndex="0" GridPane.rowIndex="1" />
    <Button fx:id="newGameButton" contentDisplay="CENTER" defaultButton="true" mnemonicParsing="false" onAction="#startNewGame" text="New Game" textAlignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="0" />
    <Button fx:id="loadGameButton" contentDisplay="CENTER" defaultButton="true" mnemonicParsing="false" onAction="#loadSavedGame" text="Load Game" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    <Button fx:id="exitGameButton" cancelButton="true" mnemonicParsing="false" onAction="#exitGame" text="Exit Game" GridPane.columnIndex="1" GridPane.rowIndex="2" />
  </children>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
  </rowConstraints>
</GridPane>
