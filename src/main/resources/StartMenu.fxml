<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane fx:id="startMenuPane" alignment="CENTER" disable="false" gridLinesVisible="false" prefHeight="650.0" prefWidth="1200.0" scaleX="1.0" styleClass="default" visible="true" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="org.csproject.view.StartMenuController">
  <children>
    <Pane id="default.css" prefHeight="720.0" prefWidth="1280.0" style="" styleClass="default" GridPane.columnIndex="0" GridPane.rowIndex="0">
      <children>
        <ImageView id="Book.png" fitHeight="720.0" fitWidth="1280.0" layoutX="0.0" layoutY="0.0">
          <image>
            <Image url="@images/titlescreens/Book.png" preserveRatio="true" smooth="true" />
          </image>
        </ImageView>
        <Label fx:id="titleLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="25.0" layoutY="60.0" prefHeight="46.0" prefWidth="357.0" styleClass="titleText" text="World Of Zennestria" textAlignment="CENTER" underline="false">
          <font>
            <Font name="Book Antiqua" size="35.0" />
          </font>
          <stylesheets>
            <URL value="@Style.css" />
          </stylesheets>
        </Label>
        <Button fx:id="loadGameButton" contentDisplay="CENTER" defaultButton="true" layoutX="942.0" layoutY="290.0" mnemonicParsing="false" onAction="#loadSavedGame" prefHeight="70.0" prefWidth="169.0" text="Load Game" textAlignment="CENTER" />
        <Button fx:id="newGameButton" contentDisplay="CENTER" defaultButton="true" layoutX="942.0" layoutY="166.0" mnemonicParsing="false" onAction="#startNewGame" prefHeight="70.0" prefWidth="169.0" text="New Game" textAlignment="CENTER" />
        <Button fx:id="exitGameButton" cancelButton="true" layoutX="942.0" layoutY="426.0" mnemonicParsing="false" onAction="#exitGame" prefHeight="70.0" prefWidth="169.0" text="Exit Game" />
      </children>
      <stylesheets>
        <URL value="@Style.css" />
      </stylesheets>
    </Pane>
  </children>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
  </rowConstraints>
</GridPane>
